{
	"compilerOptions": {
		"target": "es2016",
		"module": "commonjs",
    "baseUrl": ".",
		"rootDir": ".",
		"outDir": "./dist",
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"skipLibCheck": true
	},
	"exclude": ["**/node_modules", "**/.*/", "**/*.js", "**/dist/**/*"]
}
